{
  "name": "@fhevm/sdk",
  "version": "1.0.0",
  "description": "Universal SDK for building confidential dApps with Fully Homomorphic Encryption",
  "main": "./dist/index.js",
  "module": "./dist/index.esm.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./react": {
      "import": "./dist/react.esm.js",
      "require": "./dist/react.js",
      "types": "./dist/react.d.ts"
    }
  },
  "files": [
    "dist",
    "src",
    "README.md"
  ],
  "scripts": {
    "build": "npm run build:cjs && npm run build:esm && npm run build:types",
    "build:cjs": "tsc --module commonjs --outDir dist",
    "build:esm": "tsc --module esnext --outDir dist",
    "build:types": "tsc --declaration --emitDeclarationOnly --outDir dist",
    "dev": "tsc --watch",
    "test": "jest",
    "lint": "eslint src --ext .ts,.tsx",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "fhevm",
    "fhe",
    "encryption",
    "privacy",
    "zama",
    "ethereum",
    "sdk",
    "confidential"
  ],
  "author": "FHEVM SDK Team",
  "license": "MIT",
  "peerDependencies": {
    "ethers": "^6.0.0",
    "react": "^18.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    }
  },
  "dependencies": {
    "@fhevm/solidity": "^0.5.0",
    "fhevmjs": "^0.5.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "typescript": "^5.3.0"
  }
}
