{
  "name": "fhevm-react-template",
  "version": "2.0.0",
  "description": "Universal FHEVM SDK for building confidential dApps with Next.js, React, Vue, and Node.js",
  "private": true,
  "workspaces": [
    "packages/*",
    "examples/*"
  ],
  "scripts": {
    "install:all": "npm install",
    "build:sdk": "npm run build --workspace=packages/fhevm-sdk",
    "build:all": "npm run build:sdk && npm run build --workspaces --if-present",
    "dev:nextjs": "npm run dev --workspace=examples/nextjs-app",
    "dev:sports": "npm run dev --workspace=examples/sports-group-buying",
    "test:sdk": "npm run test --workspace=packages/fhevm-sdk",
    "test:all": "npm run test --workspaces --if-present",
    "lint": "npm run lint --workspaces --if-present",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "compile:contracts": "npm run compile --workspace=examples/sports-group-buying",
    "deploy:sports": "npm run deploy --workspace=examples/sports-group-buying",
    "clean": "npm run clean --workspaces --if-present && rm -rf node_modules"
  },
  "keywords": [
    "fhevm",
    "fhe",
    "zama",
    "privacy",
    "encryption",
    "confidential",
    "sdk",
    "react",
    "nextjs",
    "vue",
    "ethereum",
    "web3"
  ],
  "author": "FHEVM SDK Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/fhevm-react-template"
  },
  "devDependencies": {
    "prettier": "^3.2.0",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
